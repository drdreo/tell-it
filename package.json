    "scripts": {
        "build:app": "nx build tell-it --configuration=production",
        "build:server": "nx build api --configuration=production",
        "start:server:prod": "node dist/apps/api/main.js",
        "start:db": "docker run --name tell-it-DB --rm -e POSTGRES_USER=dev-user -e POSTGRES_PASSWORD=password1 -e PGDATA=/var/lib/postgresql/data/pgdata -v /tmp:/var/lib/postgresql/data -p 5432:5432 -it postgres:14-alpine",
        "test": "nx test",
        "migrate": "nx migrate latest",
        "migrate:run": "nx migrate --run-migrations",
        "db:migration:create": "typeorm migration:create ./apps/api/src/app/migrations/<migration-name>",
        "db:migration:run": "typeorm migration:run -d ./apps/api/src/app/migrations/1669146694911-CreateDB.ts"
    },
